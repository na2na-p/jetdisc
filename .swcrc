{
	"$schema": "https://json.schemastore.org/swcrc",
	"jsc": {
		"parser": {
			"syntax": "typescript",
			"dynamicImport": false,
			"decorators": true
		},
		"target": "es2022",
		"loose": false,
		"externalHelpers": false,
		"keepClassNames": false,
		"paths": {
			"@/*": ["src/*"],
			"@modules/*": ["src/modules/*"],
			"@config/*": [".config/*"],
			"@utils/*": ["src/utils/*"]
		}
	},
	"minify": false
}
